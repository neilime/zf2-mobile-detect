{
    "name": "neilime/zf2-mobile-detect",
    "type": "library",
    "description": "Zend Framework 2 module that provides Mobile-Detect features",
    "keywords": ["Zend Framework 2","mobile", "mobile detect","Mobile-Detect"],
    "homepage": "https://github.com/neilime/zf2-mobile-detect",
    "license": "MIT",
    "authors": [
        {
            "name": "Neilime",
            "homepage": "https://github.com/neilime",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/neilime/zf2-mobile-detect/issues"
    },
    "minimum-stability" : "stable",
    "require": {
        "php": ">=5.6",
		"zendframework/zend-mvc": "3.*.*",
        "mobiledetect/mobiledetectlib": "2.*.*"
    },
	"require-dev": {
        "phpunit/phpunit": "4.*.*",
		"zendframework/zend-test": "3.*.*"
    },
    "autoload": {
        "psr-4": {
            "Neilime\\MobileDetect\\": "src"
        },
        "classmap" : ["Module.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
